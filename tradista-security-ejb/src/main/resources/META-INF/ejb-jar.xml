<?xml version="1.0" encoding="UTF-8"?>
<ejb-jar xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="https://jakarta.ee/xml/ns/jakartaee"
	xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/ejb-jar_4_0.xsd"
	version="4.0">
	<module-name>security-ejb</module-name>
	<display-name>security-ejb</display-name>
	<enterprise-beans>
		<session>
			<ejb-name>BondTradeServiceBean</ejb-name>
			<ejb-class>
				org.eclipse.tradista.security.bond.service.BondTradeServiceBean</ejb-class>
			<session-type>Stateless</session-type>
			<resource-ref>
				<res-ref-name>ConnectionFactory</res-ref-name>
				<injection-target>
					<injection-target-class>org.eclipse.tradista.security.bond.service.BondTradeServiceBean</injection-target-class>
					<injection-target-name>factory</injection-target-name>
				</injection-target>
				<lookup-name>
		 java:/ConnectionFactory
		 </lookup-name>
			</resource-ref>
			<resource-ref>
				<res-ref-name>TransferQueue</res-ref-name>
				<injection-target>
					<injection-target-class>org.eclipse.tradista.security.bond.service.BondTradeServiceBean</injection-target-class>
					<injection-target-name>destination</injection-target-name>
				</injection-target>
				<lookup-name>
		 java:/jms/queue/TransferQueue
		 </lookup-name>
			</resource-ref>
		</session>
		<session>
			<ejb-name>EquityTradeServiceBean</ejb-name>
			<ejb-class>
				org.eclipse.tradista.security.equity.service.EquityTradeServiceBean</ejb-class>
			<session-type>Stateless</session-type>
			<resource-ref>
				<res-ref-name>ConnectionFactory</res-ref-name>
				<injection-target>
					<injection-target-class>org.eclipse.tradista.security.equity.service.EquityTradeServiceBean</injection-target-class>
					<injection-target-name>factory</injection-target-name>
				</injection-target>
				<lookup-name>
		 java:/ConnectionFactory
		 </lookup-name>
			</resource-ref>
			<resource-ref>
				<res-ref-name>TransferQueue</res-ref-name>
				<injection-target>
					<injection-target-class>org.eclipse.tradista.security.equity.service.EquityTradeServiceBean</injection-target-class>
					<injection-target-name>destination</injection-target-name>
				</injection-target>
				<lookup-name>
		 java:/jms/queue/TransferQueue
		 </lookup-name>
			</resource-ref>
		</session>
		<session>
			<ejb-name>EquityOptionTradeServiceBean</ejb-name>
			<ejb-class>
				org.eclipse.tradista.security.equityoption.service.EquityOptionTradeServiceBean</ejb-class>
			<session-type>Stateless</session-type>
			<resource-ref>
				<res-ref-name>ConnectionFactory</res-ref-name>
				<injection-target>
					<injection-target-class>org.eclipse.tradista.security.equityoption.service.EquityOptionTradeServiceBean</injection-target-class>
					<injection-target-name>factory</injection-target-name>
				</injection-target>
				<lookup-name>
		 java:/ConnectionFactory
		 </lookup-name>
			</resource-ref>
			<resource-ref>
				<res-ref-name>TransferQueue</res-ref-name>
				<injection-target>
					<injection-target-class>org.eclipse.tradista.security.equityoption.service.EquityOptionTradeServiceBean</injection-target-class>
					<injection-target-name>destination</injection-target-name>
				</injection-target>
				<lookup-name>
		 java:/jms/queue/TransferQueue
		 </lookup-name>
			</resource-ref>
		</session>
		<session>
			<ejb-name>GCRepoTradeServiceBean</ejb-name>
			<ejb-class>
				org.eclipse.tradista.security.gcrepo.service.GCRepoTradeServiceBean</ejb-class>
			<session-type>Stateless</session-type>
			<resource-ref>
				<res-ref-name>ConnectionFactory</res-ref-name>
				<injection-target>
					<injection-target-class>org.eclipse.tradista.security.gcrepo.service.GCRepoTradeServiceBean</injection-target-class>
					<injection-target-name>factory</injection-target-name>
				</injection-target>
				<lookup-name>
		 java:/ConnectionFactory
		 </lookup-name>
			</resource-ref>
			<resource-ref>
				<res-ref-name>TransferQueue</res-ref-name>
				<injection-target>
					<injection-target-class>org.eclipse.tradista.security.gcrepo.service.GCRepoTradeServiceBean</injection-target-class>
					<injection-target-name>destination</injection-target-name>
				</injection-target>
				<lookup-name>
		 java:/jms/queue/TransferQueue
		 </lookup-name>
			</resource-ref>
		</session>
		<session>
			<ejb-name>SpecificRepoTradeServiceBean</ejb-name>
			<ejb-class>
				org.eclipse.tradista.security.specificrepo.service.SpecificRepoTradeServiceBean</ejb-class>
			<session-type>Stateless</session-type>
			<resource-ref>
				<res-ref-name>ConnectionFactory</res-ref-name>
				<injection-target>
					<injection-target-class>org.eclipse.tradista.security.specificrepo.service.SpecificRepoTradeServiceBean</injection-target-class>
					<injection-target-name>factory</injection-target-name>
				</injection-target>
				<lookup-name>
		 java:/ConnectionFactory
		 </lookup-name>
			</resource-ref>
			<resource-ref>
				<res-ref-name>TransferQueue</res-ref-name>
				<injection-target>
					<injection-target-class>org.eclipse.tradista.security.specificrepo.service.SpecificRepoTradeServiceBean</injection-target-class>
					<injection-target-name>destination</injection-target-name>
				</injection-target>
				<lookup-name>
		 java:/jms/queue/TransferQueue
		 </lookup-name>
			</resource-ref>
		</session>
	</enterprise-beans>
	<interceptors>
		<interceptor>
			<interceptor-class>org.eclipse.tradista.core.common.service.TradistaExceptionHandlerInterceptor</interceptor-class>
		</interceptor>
	</interceptors>
	<assembly-descriptor>
		<interceptor-binding>
			<ejb-name>*</ejb-name>
			<interceptor-class>org.eclipse.tradista.core.common.service.TradistaExceptionHandlerInterceptor</interceptor-class>
		</interceptor-binding>
	</assembly-descriptor>
</ejb-jar>