<assembly>
	<id>bin</id>
	<formats>
		<format>dir</format>
	</formats>
	<dependencySets>
		<dependencySet>
			<includes>
				<include>*javafx*</include>
			</includes>
			<unpack>false</unpack>
			<scope>runtime</scope>
			<outputDirectory>lib/javafx</outputDirectory>
		</dependencySet>
		<dependencySet>
			<excludes>
				<exclude>*javafx*</exclude>
			</excludes>
			<unpack>false</unpack>
			<scope>runtime</scope>
			<outputDirectory>lib</outputDirectory>
		</dependencySet>
	</dependencySets>
	<fileSets>
		<fileSet>
			<directory>src/main/resources</directory>
			<outputDirectory></outputDirectory>
			<includes>
				<include>*.properties</include>
			</includes>
		</fileSet>
		<fileSet>
			<directory>src/main/resources/scripts</directory>
			<outputDirectory></outputDirectory>
			<includes>
				<include>*.sh</include>
				<include>*.command</include>
				<include>*.bat</include>
			</includes>
		</fileSet>
	</fileSets>
</assembly>